{
    "name": "foodticket/jet-connect-client",
    "description": "A PHP client to integrate with the JET Connect API",
    "keywords": ["PHP", "JET Connect", "JustEatTakeaway", "delivery", "api client"],
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Foodticket\\JetConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Morskate",
            "email": "rik@foodticket.nl"
        }, {
            "name": "Henrik Steffensen",
            "email": "henrik@foodticket.nl"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Foodticket\\JetConnect\\JetConnectServiceProvider"
            ]
        }
    }
}
